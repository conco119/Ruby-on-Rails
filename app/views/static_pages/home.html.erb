<% provide :title, t(".title") %>
<div class="center jumbotron">
  <h1><%= t ".welcome" %></h1>
  <h2>
    <%=
      raw t(".intro", r_tut: link_to(t(".r_tut" ), "http://rubyonrails.org/"))
    %>
  </h2>
  <p>
    <%= link_to t(".sign_up"), signup_path, class: "btn btn-lg btn-primary" %>
  </p>
</div>
<p>
  <%=
    link_to image_tag("rails.png", alt: "Rails logo"),"http://rubyonrails.org/"
  %>
</p>
